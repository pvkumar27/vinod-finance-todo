{
  "name": "fintask",
  "version": "4.3.1",
  "homepage": "https://fintask.netlify.app",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/generative-ai": "^0.24.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@netlify/functions": "^2.8.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.56.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "chrono-node": "^2.8.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "glob": "^11.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2",
    "rimraf": "^6.0.1",
    "web-push": "^3.6.7",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^7.3.0",
    "workbox-broadcast-update": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-google-analytics": "^7.3.0",
    "workbox-navigation-preload": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-range-requests": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-streams": "^7.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.4",
    "eslint-plugin-cypress": "^2.15.2",
    "husky": "^8.0.3",
    "lint-staged": "^16.1.5",
    "mochawesome": "^7.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "start": "netlify dev",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.{js,jsx} --max-warnings 0",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "prepare": "husky install",
    "test:pre-commit": "npm run test:e2e:essential",
    "test:update-expectations": "node scripts/update-test-expectations.js",

    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "test:e2e": "npx cypress run",
    "test:e2e:essential": "npx cypress run --spec 'cypress/e2e/essential.cy.js'",
    "test:e2e:auth": "npx cypress run --spec 'cypress/e2e/authentication.cy.js'",
    "test:e2e:nav": "npx cypress run --spec 'cypress/e2e/navigation.cy.js'",
    "test:e2e:ai": "npx cypress run --spec 'cypress/e2e/ai-assistant.cy.js'",
    "test:e2e:headed": "npx cypress run --headed",
    "test:e2e:chrome": "npx cypress run --browser chrome",
    "test:e2e:report": "npx cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=true,json=true",
    "eject": "react-scripts eject",
    "rollback": "node scripts/rollback.js",
    "pre-release": "node scripts/pre-release.js",
    "release": "node scripts/release.js",
    "check-updates": "npm outdated",
    "update-packages": "npm outdated && npm update && npm audit fix || echo 'All packages up to date'",
    "backup": "node scripts/backup-supabase.js",
    "restore": "node scripts/restore-backup.js",
    "test:backup": "node scripts/create-test-backup.js",
    "test:restore": "node scripts/test-restore.js",
    "backup:admin": "node scripts/backup-supabase-admin.js",
    "cleanup": "node scripts/cleanup.js",
    "mcp:setup": "node scripts/setup-mcp.js",
    "mcp:install": "cd mcp-server && npm install",
    "mcp:start": "cd mcp-server && npm start",
    "mcp:dev": "cd mcp-server && npm run dev",
    "generate-vapid": "npx web-push generate-vapid-keys"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
